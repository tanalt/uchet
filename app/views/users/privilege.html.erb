<% @users.each do |user| %>
    <%= form_for (user) do |f| %>
        <%= f.label user.name%>
        <%= f.check_box :privilege%>
        <%= f.submit "Обновити", name: "update", class: "btn btn-sm btn-primary" %>
        <%= f.submit "Видалити", name: "delete", class: "btn btn-sm btn-primary" %>
    <%end%>
<%end%>